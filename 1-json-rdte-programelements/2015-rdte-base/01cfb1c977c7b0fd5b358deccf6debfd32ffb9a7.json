                        "AccomplishmentPlannedProgramList": {
                            "AccomplishmentPlannedProgram": [
                                {
                                    "Title": {
                                        "val": "Facility restoration and modernization at the 96 TG"
                                    },
                                    "Description": {
                                        "val": "Facility restoration and modernization at the 96th TG."
                                    },
                                    "Accomplishment": {
                                        "PriorYear": {
                                            "Funding": {
                                                "val": "0.816"
                                            },
                                            "Text": {
                                                "val": "Renovated bathrooms at the Central Inertial and Global Positioning Satellite (GPS) Test Facility (CIGTF). Upgraded carpet and ceilings in information technology office.  Replaced hydraulic cylinders at National Radar Cross Section (RCS) Test Facility (NRTF).  Modernized foundations and structural components at NRTF."
                                            }
                                        }
                                    },
                                    "PlannedProgram": {
                                        "CurrentYear": {
                                            "Funding": {
                                                "val": "0.827"
                                            },
                                            "Text": {
                                                "val": "Restore deteriorated High Speed Test Track (HSTT) building. Continue bathroom renovation and install new windows in 1950s era building at the CIGTF.  Repair and improve road used to transport weapons from munitions facility to aircraft."
                                            }
                                        },
                                        "BudgetYearOne": {
                                            "Funding": {
                                                "val": "0.431"
                                            }
                                        },
                                        "BudgetYearOneBase": {
                                            "Funding": {
                                                "val": "0.431"
                                            },
                                            "Text": {
                                                "val": "Upgrade and remodel calibration pit at NRTF.  Restore and modernize 168i Dynamometer at the Landing Gear Test Facility (LGTF).  Install structures to shelter High Power Jammers at CIGTF."
                                            }
                                        }
                                    }
                                },
                                {
                                    "Title": {
                                        "val": "Facility restoration and modernization at the 96 TW"
                                    },
                                    "Description": {
                                        "val": "Facility restoration and modernization at the 96th TW."
                                    },
                                    "Accomplishment": {
                                        "PriorYear": {
                                            "Funding": {
                                                "val": "2.351"
                                            },
                                            "Text": {
                                                "val": "Repainted two Methyl Chloride Tanks at Climatic Lab.  Built two additional munitions storage igloos at Range Complex C-74.  Designed and planned relocation of High Explosive Incendiary (HEI) firing range from Range Location C-52.  Installed entry control system for the Design and Development Facility at 96 Test Support Squadron (TSS).  Installed target shelter for target support maintenance and repair.  Replaced threat seekers at the Guided Weapons Evaluation Facility (GWEF).  Replaced three radar C-Band transponders. Replaced 10 range access gates."
                                            }
                                        }
                                    },
                                    "PlannedProgram": {
                                        "CurrentYear": {
                                            "Funding": {
                                                "val": "2.387"
                                            },
                                            "Text": {
                                                "val": "Construct concrete berms under Air Management Unit-1 (AMU-1) at Climatic Lab.  Renovate Room R-139 in GWEF to support infrared (IR) test.  Install 3-phase power to Building (Bldg) 628. Replace wooden steps for Bldg 133. Replace remaining Flight Motion simulator controllers at GWEF.  Replace roof at Bldg 9307. Repair fire suppression system for Bldg 12555.  Renovate facility for Bldg 961."
                                            }
                                        },
                                        "BudgetYearOne": {
                                            "Funding": {
                                                "val": "1.210"
                                            }
                                        },
                                        "BudgetYearOneBase": {
                                            "Funding": {
                                                "val": "1.210"
                                            },
                                            "Text": {
                                                "val": "Continue Range Environmental Protection and small Restoration and Modernization (R&M) efforts across the range complex."
                                            }
                                        }
                                    }
                                },
                                {
                                    "Title": {
                                        "val": "Facility restoration and modernization at AEDC"
                                    },
                                    "Description": {
                                        "val": "Facility restoration and modernization at AEDC."
                                    },
                                    "Accomplishment": {
                                        "PriorYear": {
                                            "Funding": {
                                                "val": "31.443"
                                            },
                                            "Text": {
                                                "val": "Continued to recertify Pressure and Hazardous Material Systems (PHMS).  Continued planning for Test Utility Power System Upgrades for new supervisory control and data acquisition (SCADA) system.  Replaced C-Plant RC11 water coils, repaired trichloroethylene (TCE) coolant  lines, repair R12 freon systems, replaced reefer unit motors and gear sets.  Repaired C-Plant ventilation in Test Cells C-1 and C-2. Replaced arc heater hydrogen (H2) diffuser, upgraded H2 mixing chamber and upgraded conical high-enthalpy nozzles. Refurbished five Plenum Evacuation System compressor rotors."
                                            }
                                        }
                                    },
                                    "PlannedProgram": {
                                        "CurrentYear": {
                                            "Funding": {
                                                "val": "36.640"
                                            },
                                            "Text": {
                                                "val": "Continue FY13 efforts that were deferred or not completed.  Continue to recertify Pressure and Hazardous Material Systems (PHMS).  Procure engine test cell C-1 and C-2 digital temperature scanners.  Upgrade power distribution and control for arc heater model positioning system.  Replace Von Karman Facility (VKF) cooler #HPA-HE1-VU (111) and compressor C111.  Replace throttles in engine test cells C-1, C-2, J-1, J-2, SL-2, SL-3 and T-4. Refurbish C-Plant hydraulic systems 1-6 and 9-18."
                                            }
                                        },
                                        "BudgetYearOne": {
                                            "Funding": {
                                                "val": "43.566"
                                            }
                                        },
                                        "BudgetYearOneBase": {
                                            "Funding": {
                                                "val": "43.566"
                                            },
                                            "Text": {
                                                "val": "Continue FY14 efforts not completed or that were multiyear.  Continue upgrading control systems from analog to digital in the Exhaust Test Facility.  Continue planning to improve the exhaust plant cooling systems.  Continue work to improve the 16T wind tunnel Foreign Object Damage (FOD) fix effort.  Improve balances and calibration systems."
                                            }
                                        }
                                    }
                                },
                                {
                                    "Title": {
                                        "val": "Facility restoration and modernization at 412 TW"
                                    },
                                    "Description": {
                                        "val": "Facility restoration and modernization at the 412 TW."
                                    },
                                    "Accomplishment": {
                                        "PriorYear": {
                                            "Funding": {
                                                "val": "4.244"
                                            },
                                            "Text": {
                                                "val": "Planned replacement of the Benefield Anechoic Facility (BAF) Radar Absorbent Material (RAM) in two increments.  Replaced Integrated Facility for Avionics Systems Testing (IFAST) 400Hz frequency converters.  Replaced the Polyalphaolefin (PAO) system in Building 1030.  Converted the deluge system to a high expansion foam system."
                                            }
                                        }
                                    },
                                    "PlannedProgram": {
                                        "CurrentYear": {
                                            "Funding": {
                                                "val": "4.306"
                                            },
                                            "Text": {
                                                "val": "Continue restoring and modernizing electronic warfare and test range facilities to include projects for roofing, heating and air conditioning, windows,  doors, floors, work area re-zoning, transformers and power systems, fire suppression systems, seismic upgrades, and RAM replacement.\n\nContinue planning replacement of the BAF RAM in two increments.  Replace IFAST 400Hz frequency converters."
                                            }
                                        },
                                        "BudgetYearOne": {
                                            "Funding": {
                                                "val": "1.748"
                                            }
                                        },
                                        "BudgetYearOneBase": {
                                            "Funding": {
                                                "val": "1.748"
                                            },
                                            "Text": {
                                                "val": "Continue restoring and modernizing electronic warfare and test range facilities to include projects for roofing, heating and air conditioning, windows,  doors, floors, work area re-zoning, transformers and power systems, fire suppression systems, seismic upgrades, and RAM replacement.\n\nContinue planning replacement of the BAF RAM in two increments.  Continue replacement of IFAST 400Hz frequency converters."
                                            }
                                        }
                                    }
                                }
                            ]
