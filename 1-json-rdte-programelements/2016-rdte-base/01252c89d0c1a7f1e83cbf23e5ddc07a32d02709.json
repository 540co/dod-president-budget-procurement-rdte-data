                    "R2aExhibit": {
                        "ProjectMissionDescription": {
                            "val": "This project supports component development and Material Handling Equipment (MHE) prototyping and stays abreast of emerging and available technologies to be integrated into military MHE to address identified capability gaps and warfighter objectives.  This project enables the development of selected technologies and transition to system integration and development or production of MHE products.  MHE includes Rough Terrain Forklifts, Rough Terrain Container Handlers (RTCH) and Cranes, as well as ancillary MHE equipment, to support distribution of critical supplies in the theater of operations.  FY16 funding will allow for innovative research with the goal to improve fuel efficiency, operational energy technologies, machine diagnostics and to enhance operation of MHE equipment."
                        },
                        "AccomplishmentPlannedProgramList": {
                            "AccomplishmentPlannedProgram": [
                                {
                                    "Title": {
                                        "val": "Baseline Fuel Efficiency of Material Handling Equipment (MHE)"
                                    },
                                    "Description": {
                                        "val": "Develop standard duty cycles for fielded system, investigate training\/technology for improving efficiency and validate performance of proposed changes."
                                    },
                                    "PlannedProgram": {
                                        "CurrentYear": {
                                            "Funding": {
                                                "val": "0.250"
                                            },
                                            "Text": {
                                                "val": "Continue baseline and evaluate new solutions for fluid and controls"
                                            }
                                        },
                                        "BudgetYearOne": {
                                            "Funding": {
                                                "val": "0.060"
                                            }
                                        },
                                        "BudgetYearOneBase": {
                                            "Funding": {
                                                "val": "0.060"
                                            },
                                            "Text": {
                                                "val": "Continue baseline and evaluate new solutions for fluid and controls"
                                            }
                                        }
                                    }
                                },
                                {
                                    "Title": {
                                        "val": "Upgrade RTCH control systems and on-board diagnostics"
                                    },
                                    "Description": {
                                        "val": "Perform analysis of current control systems and technology which can be used to improve performance and reliability.  Investigate integration of on-board diagnostics to aid in operation and maintenance."
                                    },
                                    "PlannedProgram": {
                                        "CurrentYear": {
                                            "Funding": {
                                                "val": "0.434"
                                            },
                                            "Text": {
                                                "val": "Investigate current RTCH control systems and conduct Market Research"
                                            }
                                        }
                                    }
                                },
                                {
                                    "Title": {
                                        "val": "Investigate Robotic Assist on Material Handling Equipment (MHE)"
                                    },
                                    "Description": {
                                        "val": "Research and demonstrate technologies which would enhance operation such as the inclusion of cameras, collision sensors and lifting aids."
                                    },
                                    "PlannedProgram": {
                                        "CurrentYear": {
                                            "Funding": {
                                                "val": "0.150"
                                            },
                                            "Text": {
                                                "val": "Conduct research into appliqu\u00e9 which will enhance operation"
                                            }
                                        }
                                    }
                                },
                                {
                                    "Title": {
                                        "val": "Operational Energy Technologies"
                                    },
                                    "Description": {
                                        "val": "Evaluate emerging technologies that can improve machine productivity and efficiency.  Baseline fuel efficiency, engine management, efficient lubricants and hydraulic technologies."
                                    },
                                    "PlannedProgram": {
                                        "BudgetYearOne": {
                                            "Funding": {
                                                "val": "0.240"
                                            }
                                        },
                                        "BudgetYearOneBase": {
                                            "Funding": {
                                                "val": "0.240"
                                            },
                                            "Text": {
                                                "val": "Instrument up to three vehicle types (Light Capacity Rough Terrain Forklift, Rough Terrain Container Handler and All Terrain Lifting Army System) and monitor fuel consumption during operations.  Build duty cycle profiles for the classes of equipment and identify areas of inefficiency and language to include in future procurements."
                                            }
                                        }
                                    }
                                },
                                {
                                    "Title": {
                                        "val": "Machine Diagnostic"
                                    },
                                    "Description": {
                                        "val": "Evaluate machine diagnostic and condition based maintenance technologies to enhance operational availability and reduce overall maintenance burden."
                                    },
