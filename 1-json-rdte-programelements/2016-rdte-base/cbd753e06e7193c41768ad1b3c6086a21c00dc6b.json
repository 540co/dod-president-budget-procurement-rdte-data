                        "ProjectMissionDescription": {
                            "val": "This program funds ground-based optical space situational awareness (SSA) technology development and demonstration at the Maui Space Surveillance System (MSSS) in Hawaii, as well as the operation and upgrade of the facility.  Efforts in this program have been coordinated through the Department of Defense (DoD) Science and Technology (S&T) Executive Committee process to harmonize efforts and eliminate duplication.  \n\nThis program is in Budget Activity 3, Advanced Technology Development because this budget activity includes development of subsystems and components and efforts to integrate subsystems and components into system prototypes for field experiments and\/or tests in a simulated environment."
                        },
                        "AccomplishmentPlannedProgramList": {
                            "AccomplishmentPlannedProgram": [
                                {
                                    "Title": {
                                        "val": "Operate and Upgrade Maui Space Surveillance System (MSSS)"
                                    },
                                    "Description": {
                                        "val": "Operate and upgrade the MSSS to support development, demonstration, and integration of ground-based optical SSA technologies."
                                    },
                                    "Accomplishment": {
                                        "PriorYear": {
                                            "Funding": {
                                                "val": "20.166"
                                            },
                                            "Text": {
                                                "val": "Maintained MSSS facility and experimental equipment in a mission-ready state.  Performed needed upgrades and modernization to keep facilities and equipment in good working order and allow MSSS to perform efficiently and reliably including completing refurbishment of 1.6 meter dome.  Completed delivery and began installation of the upgraded Laser Guidestar system at MSSS.  Implemented procedures and operated MSSS facility for development and demonstration of ground based optical SSA capabilities in conjunction with customer programs and an operational SSA mission."
                                            }
                                        }
                                    },
                                    "PlannedProgram": {
                                        "CurrentYear": {
                                            "Funding": {
                                                "val": "11.002"
                                            },
                                            "Text": {
                                                "val": "Maintain MSSS facility and experimental equipment in a mission-ready state.  Perform needed upgrades and modernization to keep facilities and equipment in good working order and allow MSSS to perform efficiently and reliably.  Operate MSSS facility for development and demonstration of ground based optical SSA capabilities in conjunction with customer programs and an operational SSA mission.  Complete installation and testing of equipment for upgraded Laser Guidestar system at MSSS."
                                            }
                                        },
                                        "BudgetYearOne": {
                                            "Funding": {
                                                "val": "10.853"
                                            }
                                        },
                                        "BudgetYearOneBase": {
                                            "Funding": {
                                                "val": "10.853"
                                            },
                                            "Text": {
                                                "val": "Maintain MSSS facility and experimental equipment in a mission-ready state.  Perform needed upgrades and modernization to keep facilities and equipment in good working order and allow MSSS to perform efficiently and reliably.  Operate MSSS facility for development and demonstration of ground based optical SSA capabilities in conjunction with customer programs and an operational SSA mission.  Demonstrate operation of Laser Guidestar system at MSSS."
                                            }
                                        }
                                    }
                                },
                                {
                                    "Title": {
                                        "val": "Geosynchronous Object Sensor"
                                    },
                                    "Description": {
                                        "val": "Develop and demonstrate dual-use integrated sensor technology for imaging of geosynchronous objects as well as other long-range applications."
                                    },
                                    "Accomplishment": {
                                        "PriorYear": {
                                            "Funding": {
                                                "val": "5.369"
                                            },
                                            "Text": {
                                                "val": "Continued to develop laser detection and ranging transceiver for initial low-power technology demonstration to image objects in geosynchronous orbit."
                                            }
                                        }
                                    },
                                    "PlannedProgram": {
                                        "CurrentYear": {
                                            "Funding": {
                                                "val": "3.029"
                                            },
                                            "Text": {
                                                "val": "Integrate low power transceiver hardware prior to low power testing to image objects in geosynchronous orbit."
                                            }
                                        },
                                        "BudgetYearOne": {
                                            "Funding": {
                                                "val": "2.000"
                                            }
                                        },
                                        "BudgetYearOneBase": {
                                            "Funding": {
                                                "val": "2.000"
                                            },
                                            "Text": {
                                                "val": "Conduct low power demonstration of augmented objects for refinement of high power system.  Begin build-out of high-power transceiver hardware and initiate telescope modifications for future demonstration of laser imaging of objects in geosynchronous orbit."
                                            }
                                        }
                                    }
