                        "AccomplishmentPlannedProgramList": {
                            "AccomplishmentPlannedProgram": [
                                {
                                    "Title": {
                                        "val": "Software Simulation Algorithm Maturity"
                                    },
                                    "Description": {
                                        "val": "The program will use a Modeling and Simulation (M&S) strategy that provides an Integrated Flight Simulation (IFS), a software test station, and a high fidelity hardware in the loop simulation facility."
                                    },
                                    "Accomplishment": {
                                        "PriorYear": {
                                            "Funding": {
                                                "val": "6.297"
                                            },
                                            "Articles": {
                                                "val": "0"
                                            },
                                            "Text": {
                                                "val": "The IFS results were used for analysis in support of the JAGM AOA conducted by TRAC, sensitivity studies in support of CAIV analysis for millimeter wave performance, and common scene generation updates."
                                            }
                                        }
                                    }
                                },
                                {
                                    "Title": {
                                        "val": "Technology Development Phase Exit Criteria"
                                    },
                                    "Description": {
                                        "val": "The JAGM Product Management Office are developing exit criteria appropriate to the next phase or effort of the program. The OIPT will review the proposed exit criteria and make a recommendation to the Milestone Decision Authority."
                                    },
                                    "Accomplishment": {
                                        "PriorYear": {
                                            "Funding": {
                                                "val": "5.047"
                                            },
                                            "Articles": {
                                                "val": "0"
                                            },
                                            "Text": {
                                                "val": "Completing JAGM regulatory and statutory documentation in support of TD exit criteria and Source Selection Evaluation Board (SSEB) closeout."
                                            }
                                        }
                                    }
                                },
                                {
                                    "Title": {
                                        "val": "Development Activities for Subsystem Design"
                                    },
                                    "Description": {
                                        "val": "The JAGM prime contractors, with government oversight, will initiate major component subsystem design reviews and testing activities."
                                    },
                                    "Accomplishment": {
                                        "PriorYear": {
                                            "Funding": {
                                                "val": "34.911"
                                            },
                                            "Articles": {
                                                "val": "0"
                                            },
                                            "Text": {
                                                "val": "The JAGM Product Management Office is initiating Cost as An Independent Variable (CAIV) trades in support of an extended Technology Development Phase."
                                            }
                                        }
                                    }
                                },
                                {
                                    "Title": {
                                        "val": "Continued Technology Development Phase"
                                    },
                                    "Description": {
                                        "val": "The Technology Development phase will design, build, test and demonstrate seekers from both contractors to validate that both the seeker capabilities and affordability meet user requirements."
                                    },
                                    "Accomplishment": {
                                        "PriorYear": {
                                            "Funding": {
                                                "val": "25.505"
                                            },
                                            "Articles": {
                                                "val": "0"
                                            },
                                            "Text": {
                                                "val": "Develop and provide demonstration of an improved capabilities over legacy laser missiles (countermeasure, adverse weather, UAS lethality against HVT and Fire & Forget)."
                                            }
                                        }
                                    }
                                },
                                {
                                    "Title": {
                                        "val": "Delta PDR and Component Qualification"
                                    },
                                    "Description": {
                                        "val": "A cornerstone of the extended TD phase includes completing all component qualifications and performing a cost analysis on requirements prior to entering CDR.  Component qualification will give the JAGM program confidence in the JAGM system's design maturity and reduce risk for the subsystem qualification phase and pilot production line activities."
                                    },
                                    "PlannedProgram": {
                                        "CurrentYear": {
                                            "Funding": {
                                                "val": "55.105"
                                            },
                                            "Articles": {
                                                "val": "0"
                                            },
                                            "Text": {
                                                "val": "Conduct component affordability analysis, order hardware, and initiate component qualification efforts."
                                            }
                                        }
                                    }
                                },
                                {
                                    "Title": {
                                        "val": "Engineering Design Review & Subsystem Qualification"
                                    },
                                    "Description": {
                                        "val": "Complete detailed engineering design and development activities to support component and subsystem design reviews, and initiate subsystem qualification testing with emphasis on affordability."
                                    },
                                    "PlannedProgram": {
                                        "CurrentYear": {
                                            "Funding": {
                                                "val": "47.803"
                                            },
                                            "Articles": {
                                                "val": "0"
                                            },
                                            "Text": {
                                                "val": "Purchase subsystem hardware and initiate component and subsystem design reviews, testing, and platform integration activities."
                                            }
                                        }
                                    }
                                },
                                {
                                    "Title": {
                                        "val": "Integrated Baseline Review (IBR)"
                                    },
                                    "Description": {
                                        "val": "JAGM Product Office and Prime Contractor will review the entire TD affordability objectives and technical scope of work (SOW) .  The JAGM IBR team and prime will develop and implement the technical plan.  This will lead to an assessment of JAGM technical risk and allocation of resources necessary to meet the technical requirements within the schedule."
                                    },
                                    "PlannedProgram": {
                                        "CurrentYear": {
                                            "Funding": {
                                                "val": "3.845"
                                            },
                                            "Articles": {
                                                "val": "0"
                                            },
                                            "Text": {
                                                "val": "JAGM IBR team conducts Performance Measurement Baseline reviews. The reviews will tracks to the Budgeted Cost of Work Scheduled. The team will conduct reviews with the prime contractors."
                                            }
                                        }
                                    }
                                },
                                {
                                    "Title": {
                                        "val": "Sub-System Qualification"
                                    },
                                    "Description": {
                                        "val": "The JAGM PO will initiate subsystem performance and qualification testing of the JAGM missile subsystem and performance verification of the system."
                                    },
                                    "PlannedProgram": {
                                        "CurrentYear": {
                                            "Funding": {
                                                "val": "20.142"
                                            },
                                            "Articles": {
                                                "val": "0"
                                            },
                                            "Text": {
                                                "val": "The JAGM Product Office will support affordability performance and qualification testing at the subsystem level."
                                            }
                                        }
                                    }
                                },
                                {
                                    "Title": {
                                        "val": "Affordability Activities to Support Critical Design Review (CDR)"
                                    },
                                    "Description": {
                                        "val": "During the 27-month TD phase affordability extension, the JAGM Product Office will complete all major component subsystem design reviews, testing, and platform affordability activities."
                                    },
                                    "PlannedProgram": {
                                        "BudgetYearOne": {
                                            "Funding": {
