                                            "Funding": {
                                                "val": "2.048"
                                            },
                                            "Text": {
                                                "val": "Continue development and testing of Version 2.0 for both On-Scene Triage and Truecrypt Detect tools  for LE\/CI communities and NMEC."
                                            }
                                        }
                                    }
